<form method="post" action="/teams">
  
  <h1>Create a Team and Heros!</h1>
  
  <label for="team_name">Team Name:</label>
  <input type="text" name="team[name]">
  <label for="team_motto">Team Motto:</label>
  <input type="text" name="team[motto]">
  
  <h2>Hero 1</h2>
  
    <label for="hero-name">Hero's Name:</label>
  <input type="text" name="team[heros][][name]">
  <label for="hero_power">Hero's Power:</label>
  <input type="text" name="team[heros][][power]]">
  
  
  
</form>
